alias ddrive='cd /Volumes/dd'

#place photon in dfu without buttons
alias dfu='x=`compgen -f -- "/dev/tty.usbmodem"`;if [ -z "$x" ]; then echo "No USB device found"; else eval $(stty -f $x 14400); fi'

#particle CLI
alias p='particle'
alias pbi='particle serial inspect'
alias pc='particle compile'
alias pce='particle compile electron'
alias pci='particle config identify'
alias pcp='particle compile photon'
alias pf='particle flash'
alias pfc='particle function call'
alias pfu='particle flash --usb'
alias pi='particle identify'
alias pl='particle list'
